\inst new (Register dst, Value con)

\when con:function \{
  JSValue obj;
  save_context();
  obj = create_simple_object_with_constructor(context, con);
#ifdef ALLOC_SITE
  object_set_alloc_site(obj, &insns->alloc_site);
#endif /* ALLOC_SITE */
  dst = obj;
  update_context();
\}

\otherwise \{
dst = JS_UNDEFINED;
