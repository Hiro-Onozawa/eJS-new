\inst tailsend (fn)

\when fn:function \{
  set_fp(context, fp);
  set_pc(context, pc);
  tailcall_function(context, fn, nargs, TRUE);
  update_context();
  NEXT_INSN_NOINCPC();
\}

\when fn:builtin \{
  set_fp(context, fp);
  set_pc(context, pc);
  tailcall_builtin(context, fn, nargs, TRUE, FALSE);
  update_context();
  NEXT_INSN_INCPC();
\}

\otherwise \{
  LOG_EXIT("TAILSEND: not a function\n");
\}
